<?xml version="1.0" ?>
<robot name="testbot">

  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="orange">
    <color rgba="1.0 0.4 0.0 1.0"/>
  </material>

  <link name="base">
    <visual>
        <geometry>
            <cylinder length="0.02" radius="0.07"/>
        </geometry>
        <origin rpy="0 0 0" xyz="0 0 0.01"/>
        <material name="yellow">
            <color rgba="1 1 0 1"/>
        </material>
    </visual>

    <collision>
        <geometry>
            <cylinder length="0.02" radius="0.07"/>
        </geometry>
        <origin rpy="0 0 0" xyz="0 0 0.01"/>
    </collision>

      <inertial>
        <mass value="1"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>       
      </inertial>
  </link>




  <joint name="joint1" type="continuous">
    <parent link="base"/>
    <child link="link1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 0"/>
    <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
  </joint>


  <link name="link1">
    <collision>
      <origin xyz="0 0 0.17" rpy="0 0 0"/>
      <geometry>
        <cylinder length="2" radius="0.015"/>
      </geometry>
    </collision>
    <visual>
	    <origin xyz="0 0 0.17" rpy="0 0 0"/>
      <geometry>
        <cylinder length="2" radius="0.015"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>

<joint name="fixed" type="fixed">
    <parent link="link1"/>
    <child link="link2"/>
  </joint>

  <link name="link2">
    <collision>
      <origin xyz="0.02 0 0.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.12 0.07 0.03"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0.02 0 0.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.12 0.07 0.03"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>

  <joint name="joint2" type="fixed">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 0"/>
    <limit effort="30" lower="-1.317" upper="1.687" velocity="1.571"/>
  </joint>

  <link name="link3">
    <collision>
      <origin xyz="0 0 0.1" rpy="0 1.57 0"/>
      <geometry>
        <cylinder length="0.2" radius="0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0.1" rpy= "0 1.57 0"/>
      <geometry>
        <cylinder length="0.2" radius="0.01"/>
      </geometry>
      <material name="red">
          <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

</robot>