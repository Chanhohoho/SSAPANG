<launch>

<include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find ssapang_map)/worlds/ssapang_center.world"/>
    <!-- <arg name="world_name" value="$(find ssapang_map)/worlds/ssapang_center.world"/> -->
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
  <!-- BEGIN BURGER -->

  <node pkg='ssapang' type='tower' name='tower' output='screen' />
  <group ns="burger1">
      <include file="$(find ssabot)/launch/burger_spawn.launch">
          <arg name="init_pose" value="-x 0 -y 0 -z 0 -Y 3.14"/>
          <arg name="robot_name" value="burger1"/>
      </include>
  </group>

  <group ns="burger2">
      <include file="$(find ssabot)/launch/burger_spawn.launch">
          <arg name="init_pose" value="-x 1 -y 0 -z 0 -Y 3.14"/>
